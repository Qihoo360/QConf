AT_SETUP([fetch from a read-only 1.8-style database])
AT_KEYWORDS([dbm fetch fetch03 dbmfetch03])

AT_CHECK([
AT_COMPAT_PREREQ
mkdir dir
cd dir
num2word 1:10 | dtload test
rm test.dir
ln test.pag test.dir
chmod -w test.dir test.pag .
dtfetch test 6 10
],
[0],
[six
ten
])

AT_CLEANUP

